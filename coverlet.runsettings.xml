<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat Code Coverage">
         <Configuration>
            <Exclude>
            [AmeriCorps.Users.Api]*Program,
            [AmeriCorps.Users.Data]*Repository,
            [AmeriCorps.Users.Data.Core]*NpgsqlContext,
            [AmeriCorps.Users.Api]*.Configuration.*,
            [AmeriCorps.Users.Api]*.Http.*,
            [AmeriCorps.Users.Api]*.Scheduler.*
            </Exclude>
          </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>